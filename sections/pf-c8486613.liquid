        var defaultScene = function() {
            return scenes.filter(scene => scene.isDefault == true)[0];
        }

        /*
            Adds a method `heightForWidth` to each scene.
            This method calculates the height for a scene based on the width it needs to fit into.
            This allows the video to be resized to fit the entire screen properly on any device.

            NOTE: Because all scenes at the moment have the same aspect ratio, this function doesn't really need to be a method on scene.
        */
        scenes.forEach(function (scene) {
            Object.getPrototypeOf(scene).heightForWidth = function(width) {
                return width * (this.height/this.width);
            }
        });

        /*
            Set up an object that we can use to handle the state of any scene
            such as starting, pausing etc.
        */
        var newStateFromScene = function(scene) {
            return {
                scene: scene,
                audioKey: 'normal',
                paused: true,
                audioFiles: {
                    normal: document.getElementById('audio-normal'),
                    eargasm: document.getElementById('audio-eargasm'),
                    foam: document.getElementById('audio-foam')
                },
                video: document.getElementById('scene-video'),
                currentAudio: function() {
                    return this.audioFiles[this.audioKey];
                },
                start: function() {
                    this.currentAudio().play();
                    this.video.play();
                    this.paused = false;

                    let normalButton = document.getElementById('audio-selector-normal');
                    let event = normalButton['click'].call(normalButton);
                    
                    this.video.addEventListener('ended', () => {
                        this.currentAudio().currentTime = 0;
                        this.video.currentTime = 0;
                        this.video.play();
                    });
                },
                playAudioType: function(key) {
                    this.currentAudio().pause();
                    this.audioKey = key;
                    this.currentAudio().currentTime = this.video.currentTime;
                    this.currentAudio().play();
                    this.paused = false;
                    document.getElementById('db-level').innerText = this.scene.dbs[key] + ' dB';
                    document.getElementById('sound-quality').innerText = this.scene.soundQuality[key];
                },
                pause: function() {
                    this.currentAudio().pause();
                    this.video.pause();
                    this.paused = true;
                    document.getElementById('icon-pause').classList.add('hidden');
                    document.getElementById('icon-play').classList.remove('hidden');
                },
                play: function() {
                    this.currentAudio().play();
                    this.video.play();
                    this.paused = false;
                    document.getElementById('icon-pause').classList.remove('hidden');
                    document.getElementById('icon-play').classList.add('hidden');
                },
                isPaused: function() {
                    return this.paused;
                }
            }
        }
       
        /*
            Presents the scene selector.
        */
        var showSceneMenu = function () {
            document.getElementById('menu-container').classList.add('show');
            document.getElementById('menu-container').classList.remove('hidden');
            
            setTimeout(function() {
                document.getElementById('menu-container').classList.remove('show');
            }, 400);
        }

        /*
            Dynamically build the scene menu from the scenes array.

            @param {array} scenes - The scenes array used to build the menu.
        */
        var buildSceneMenu = function(scenes) {
            document.getElementById('scene-menu').appendChild(
            buildMenuSection(scenes.filter(scene => scene.section === 'popular'), 'popular', ['scene-section--popular'])
            );
            
            document.getElementById('scene-menu').appendChild(
            buildMenuSection(scenes.filter(scene => scene.section === 'other'), 'bonus')
            );
        }
        
        /*
            Build a section of the scene selection menu.

            @param {array} scenes - An array of scenes to include in this menu section.
            @param {string} title - The title of this menu section.
            @para {array} classes - Any classes that will be added to the section.
        */
        var buildMenuSection = function(scenes, title, classes = []) {
            classes.push('scene-section');
            
            var section = document.createElement('div');
            classes.forEach(function(element) {
                section.classList.add(element);
            });
            
            
            var header = document.createElement('header');
            header.innerText = title;
            section.appendChild(header);
            
            var ul = document.createElement('ul');
            section.appendChild(ul);
            
            scenes.forEach(function(scene) {
                li = document.createElement('li');
                li.innerText = scene.title;
                li.addEventListener('click', didSelectSceneElement);
                li.setAttribute('scene-id', scene.id);
                ul.appendChild(li);
            });
            
            return section;
        }
        
        /*
            When the user selects one of the scenes in the scene menu.
            Hide the menu, build a new scene state and start the scene.

            @param {Object} event - The click event that triggered this.
        */
        var didSelectSceneElement = function(event) {
            /* Hide Menu*/
            document.getElementById('menu-container').classList.add('hide');
            setTimeout(function () {
                document.getElementById('menu-container').classList.add('hidden');
                document.getElementById('menu-container').classList.remove('hide');
            }, 300);

            var scene = scenes.find(scene => scene.id == event.currentTarget.getAttribute('scene-id'));

            playScene(scene);
        }

        /*
            Start playing the video from the scene.

            @param {Object} scene - The scene to play.
        */
        var playScene = function(scene) {
            if (! scene) {
                scene = defaultScene();
            }

            document.querySelectorAll('.audio-selector').forEach(function (element) {
                element.addEventListener('click', toggleAudio)
            });

            document.getElementById('button-change-scene').classList.remove('hidden');
            document.getElementById('button-play').classList.add('hidden');

            document.getElementById('scene-meta-header').classList.add('hidden');
            // fbq('trackCustom', 'AR:select-scene', { scene_title : scene.fb_title });
            document.state = newStateFromScene(scene);
            document.getElementById('scene-video').src = scene.video_file;

            ['normal', 'eargasm', 'foam'].forEach(function(type) {
                document.getElementById('audio-'+type).src = scene.audio_files[type];
            });
            prepare(document.state, function() {
                document.state.start();
                
                // clearTimeout(document.currentTimer);
                // document.currentTimer = setTimeout(function() {
                //     fbq('trackCustom', 'AR:watch-scene', { duration_in_seconds : 15, scene_title: document.state.scene.fb_title } );
                // }, 15000);

                setTimeout(function() {
                    var width = document.getElementById('scene-container').clientWidth;
                    var height = document.state.scene.heightForWidth(width);
                    document.getElementById('scene-video').style['height'] = height + 'px';
                    document.getElementById('scene-meta-header').classList.remove('hidden');
                    document.getElementById('db-level').innerText = document.state.scene.dbs['normal'] + ' dB';
                    document.getElementById('sound-quality').innerText = document.state.scene.soundQuality['normal'];
                }, 200);

                document.getElementById('scene-video').removeEventListener('click', playScene);
                document.getElementById('scene-video').addEventListener('click', togglePlayback);
                document.getElementById('scene-meta-header').addEventListener('click', togglePlayback);
            });
        }

        /*
            Pause and play the video. If there is no scene prepared, prepare the default scene and start it.
        */
        var togglePlayback = function() {
            if (! document.state) {
                return playScene();
            }

            document.state.isPaused() ? document.state.play() : document.state.pause(); 
        }

        /*
            User did select normal | eargasm | or foam audio selector.
            Use the button event to determine which audio file to start playing.

            @param {Object} event - The click event that triggered this function.
        */
        var toggleAudio = function(event) {
            if (document.state.isPaused()) { 
                document.state.play();
            }

            var regex = new RegExp("audio-selector-", 'g');
            var audioType = event.currentTarget.id.replace(regex, '');

            document.querySelectorAll('.audio-selector').forEach(function (element) {
                element.classList.remove('audio-selector--selected');
            });
            document.querySelectorAll('.audio-selector-group').forEach(function (element) {
                element.classList.remove('audio-selector-group--selected');
            });

            event.currentTarget.classList.add('audio-selector--selected');
            event.currentTarget.parentNode.classList.add('audio-selector-group--selected')

            document.state.playAudioType(audioType);
        }

        /*
            This is very important for mobile browsers. In order for this to work we
            have to first play all of the audio files and video file to get them to load.
            Once they are all buffering we can start the interactive experience.
        */
        var prepare = function(state, ready, delay = 200) {
            var audioFiles = Object.values(state.audioFiles);

            return {
                audioFiles: audioFiles,
                ready: ready,
                play: function(audio) { audio.play(); },
                pause: function(audio) { audio.pause(); },
                reset: function (audio) { audio.currentTime = 0; },
                pauseAllAudio: function() { this.audioFiles.forEach(this.pause); },
                playAllAudio: function() { 
                    document.state.video.play();
                    this.audioFiles.forEach(this.play);
                },
                resetAllAudio: function() {
                    return function() {
                        this.pauseAllAudio();
                        this.audioFiles.forEach(this.reset);
                        document.state.video.pause();
                        setTimeout(() => {
                            document.state.video.currentTime = 0;
                            document.getElementById('scene-video').classList.remove('hidden');
                            document.getElementById('loading').classList.add('hidden');
                            document.getElementById('loading').classList.remove('bounce');
                            this.ready();
                        }, 2000);
                    }
                },
                start: function(delay) {
                    document.getElementById('scene-video').classList.add('hidden');
                    document.getElementById('loading').classList.remove('hidden');
                    document.getElementById('loading').classList.add('bounce');
                    this.playAllAudio();
                    setTimeout(this.resetAllAudio.call(this, ready).bind(this), delay);
                    return this;
                }
            }.start(delay);
        }

        /*
            Document is ready.
        */
        ready(function() {
            buildSceneMenu(scenes);
            var width = document.getElementById('scene-container').clientWidth;
            var height = scenes[0].heightForWidth(width);
            document.getElementById('scene-video').style['height'] = height + 'px';

            document.getElementById('scene-video').src = defaultScene().video_file;
            document.getElementById('scene-video').poster = defaultScene().poster;

            document.getElementById('scene-video').addEventListener('click', togglePlayback);
        });
    </script>
